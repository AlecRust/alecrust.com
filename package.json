{
  "name": "alecrust.com",
  "version": "1.0.0",
  "private": true,
  "author": "Alec Rust (https://www.alecrust.com/)",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "webpack serve",
    "build": "webpack --mode production",
    "lint": "prettier --check . && eslint . && stylelint '**/*.css'",
